 171               	;   argPtr  r24+25 / r16+r17
 172               	;   argLen  r22 / r18
 173               	; temp variables:
 174               	;   byte    r18 / r22
 175               	;   scratch r23
 176               	;   resCrc  r24+r25 / r16+r17
 177               	;   ptr     X / Z
